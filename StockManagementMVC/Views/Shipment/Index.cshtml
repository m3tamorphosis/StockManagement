@model ShipmentViewModel

<form asp-action="UploadFile" enctype="multipart/form-data" >
    <div class="form-group">
        <label for="file"></label>
        <input type="file" id="file" name="File" accept="text/json" class="form-control" />
    </div>
    <input type="submit" class="btn btn-secondary" value="Submit" asp-controller="Shipment" asp-action="ShipmentDetails" />

</form>